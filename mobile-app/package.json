{
  "name": "paww-mobile",
  "version": "1.0.0",
  "homepage": "https://lucs439.github.io/paww-project",
  "main": "index.js",
  "scripts": {
    "start": "npx expo start",
    "start:offline": "npx expo start --offline",
    "start:tunnel": "npx expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:dev": "eas build --profile development --platform all",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:snapshot": "APP_ENV=snapshot eas build --profile snapshot --platform all",
    "build:snapshot:ios": "APP_ENV=snapshot eas build --profile snapshot --platform ios",
    "build:snapshot:android": "APP_ENV=snapshot eas build --profile snapshot --platform android",
    "build:prod": "APP_ENV=production eas build --profile production --platform all",
    "build:prod:ios": "APP_ENV=production eas build --profile production --platform ios",
    "build:prod:android": "APP_ENV=production eas build --profile production --platform android",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "update:snapshot": "APP_ENV=snapshot eas update --branch snapshot",
    "update:prod": "APP_ENV=production eas update --branch production"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/stack": "^7.3.3",
    "axios": "^1.7.2",
    "expo": "~53.0.9",
    "expo-constants": "^17.0.3",
    "expo-dev-client": "~5.1.8",
    "expo-font": "^13.3.1",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "^15.12.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/ngrok": "^4.1.0"
  },
  "private": true
}
